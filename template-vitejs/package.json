    "tiny-cookie": "^2.3.2",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.3.0",
    "@mui/material": "^5.6.1",
    "@sentry/react": "^6.3.6",
    "@sentry/tracing": "^6.3.6",
    "react-toastify": "^8.2.0",
    "http-status": "^1.5.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
